  <style>
    *#loader {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background-color: rgba(255, 255, 255, 0.7);
      display: flex;
      flex-direction: column;
      /* Stack spinner and percentage */
      align-items: center;
      justify-content: center;
      z-index: 1000;
    }

    #loader {
      margin-top: 1rem;
      font-size: 1.2rem;
      color: #007bff;
    }

    .spinner {
      width: 40px;
      height: 40px;
      border: 4px solid rgba(0, 0, 0, 0.1);
      border-top-color: #007bff;
      border-radius: 50%;
      animation: spin 1s linear infinite;
    }

    #loader-percentage {
      margin-top: 10px;
      font-size: 16px;
      font-weight: bold;
      color: #007bff;
    }

    @keyframes spin {
      to {
        transform: rotate(360deg);
      }
    }
  </style>
